{
  "aws_access_key": "$AWS_ACCESS_KEY_ID",
  "aws_secret_key": "$AWS_SECRET_ACCESS",
  "aws_region": "$AWS_REGION",
  "vpc_name": "$NAME-vpc",
  "vpc_cidr": "$VPC_CIDR_IP/16",
  "vpc_availability_zones": {
    "$AWS_REGIONa": {
      "public_subnet_cidr": "$VPC_CIDR_IP_16b_PREFIX.0.0/22",
      "private_subnets_cidr": [
        "$VPC_CIDR_IP_16b_PREFIX.4.0/22",
        "$VPC_CIDR_IP_16b_PREFIX.8.0/22"
      ],
      "tags": {}
    },
    "$AWS_REGIONb": {
      "public_subnet_cidr": "$VPC_CIDR_IP_16b_PREFIX.12.0/22",
      "private_subnets_cidr": [
        "$VPC_CIDR_IP_16b_PREFIX.16.0/22",
        "$VPC_CIDR_IP_16b_PREFIX.20.0/22"
      ],
      "tags": {}
    }
  },
  "vpc_pcxs": [
    {
      "vpc_id": "$PEER_VPC_ID",
      "peer_route_ids": [
        "$PEER_VPC_RTB_ID"
      ]
    }
  ],
  "vpc_tags": {}
}
